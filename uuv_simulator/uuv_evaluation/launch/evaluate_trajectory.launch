<launch>
  <arg name="rosbag" default="$(find uuv_evaluation)/bags/recording.bag"/>
  <arg name="output_dir" default="$(find uuv_evaluation)/bags"/>
  
  <node name="trajectory_eval"
        pkg="uuv_evaluation"
        type="trajectory_eval.py"
        output="screen"
        required="true"
        args="$(arg rosbag) $(arg output_dir)"/>
</launch>
